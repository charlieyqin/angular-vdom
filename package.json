{
  "name": "angular-vdom",
  "version": "0.4.1",
  "description": "angular wrapper for high performance rendering virtual-dom with the .component() method",
  "main": "index.js",
  "scripts": {
    "build": "browserify ./example/angular-app.js > ./example/built.js",
    "test": "./node_modules/.bin/karma start",
    "test-watch": "./node_modules/.bin/karma start --single-run=false",
    "watch": "watchify ./website/app.js -d -o ./website/built.js -v"
  },
  "keywords": [
    "angular",
    "virtual-dom",
    "virtual",
    "dom",
    "angular virtualdom",
    "angular vdom",
    "vdom"
  ],
  "author": "Jack Hanford <jackhanford@gmail.com> (http://jackhanford.com)",
  "license": "MIT",
  "dependencies": {
    "defaults": "~1.0.3",
    "foreach": "~2.0.5",
    "main-loop": "~3.2.0",
    "virtual-dom": "~2.1.1"
  },
  "devDependencies": {
    "angular": "~1.5.5",
    "angular-mocks": "~1.5.x",
    "browserify": "~13.0.1",
    "chai": "~3.0.0",
    "karma": "~0.12.36",
    "karma-browserify": "~4.2.1",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~0.1.12",
    "karma-mocha": "~0.1.10",
    "mocha": "~2.2.5",
    "watchify": "~3.7.0"
  }
}
